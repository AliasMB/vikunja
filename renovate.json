{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "labels": ["dependencies"],
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "matchPackageNames": ["netlify-cli", "caniuse-lite"],
      "extends": ["schedule:weekly"]
    },
    {
      "groupName": "vueuse",
      "matchPackagePrefixes": [
        "@vueuse/"
      ]
    }
  ]
}
